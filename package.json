{
  "name": "geekymedic-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s -o --proxy-config proxy/proxy.conf.json --host=0.0.0.0 --port=4200",
    "start-test": "ng s -o --proxy-config proxy/proxy.conf.test.json --host=0.0.0.0 --port=4200",
    "start-debug": "ng s -o --proxy-config proxy/proxy.conf.debug.json --host=0.0.0.0 --port=4200",
    "hmr": "ng s -o -c=hmr --proxy-config proxy/proxy.conf.json --host=0.0.0.0 --port=4200",
    "hmr-test": "ng s -o -c=hmr --proxy-config proxy/proxy.conf.test.json --host=0.0.0.0 --port=4200",
    "hmr-debug": "ng s -o -c=hmr --proxy-config proxy/proxy.conf.debug.json --host=0.0.0.0 --port=4200",
    "build": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod",
    "build-test": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build -c=test",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "analyze": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --stats-json && npm run analyze-report",
    "analyze-report": "webpack-bundle-analyzer --port 8123 dist/geekymedic-project/stats-es2015.json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "node scripts/color-less.js",
    "icon": "ng g ng-alain:plugin icon",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.3",
    "@angular/common": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/core": "~9.1.3",
    "@angular/forms": "~9.1.3",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/router": "~9.1.3",
    "@delon/abc": "^9.0.1",
    "@delon/acl": "^9.0.1",
    "@delon/auth": "^9.0.1",
    "@delon/cache": "^9.0.1",
    "@delon/chart": "^9.0.1",
    "@delon/form": "^9.0.1",
    "@delon/mock": "^9.0.1",
    "@delon/theme": "^9.0.1",
    "@delon/util": "^9.0.1",
    "@geekymedic/common": "^9.0.0",
    "@geekymedic/core": "^9.0.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "ajv": "^6.12.0",
    "lodash": "^4.17.15",
    "ng-zorro-antd": "^9.1.0",
    "rxjs": "~6.5.4",
    "screenfull": "^5.0.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/language-service": "~9.1.3",
    "@angularclass/hmr": "^2.1.3",
    "@delon/testing": "^9.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.150",
    "@types/node": "^12.11.1",
    "antd-theme-generator": "^1.1.9",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.1.2",
    "ng-alain": "^9.0.1",
    "ng-alain-codelyzer": "^0.0.1",
    "prettier": "^2.0.4",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.3",
    "stylelint": "^13.3.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "husky": {}
}
